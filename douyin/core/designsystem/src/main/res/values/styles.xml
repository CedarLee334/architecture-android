<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="douyin_core_designsystem_window_animation_bottom">
        <item name="android:windowEnterAnimation">@anim/douyin_core_designsystem_slide_in_bottom
        </item>
        <item name="android:windowExitAnimation">@anim/douyin_core_designsystem_slide_out_bottom
        </item>
    </style>

    <style name="douyin_core_designsystem_text_button">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:insetLeft">0dp</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetRight">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:clickable">false</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?attr/colorOnSurface</item>
    </style>
</resources>